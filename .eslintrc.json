{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "airbnb",
    "prettier",
    "plugin:node/recommended"
  ],
  "rules": {
    "no-unused-vars": "warn",
    "func-names": "warn",
    "no-process-exit": "warn",
    "object-shorthand": "warn",
    "class-methods-use-this": "error",
    "strict": "off",
    "no-console": "off",
    "no-plusplus": "off"
  }
}
